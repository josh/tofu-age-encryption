exec sh -c 'tofu-age-encryption --help | sed -E "s|default \"[^\"]*age\"|default \"@AGE_PROGRAM@\"|"'
cmp stdout help.txt
! stderr .

-- help.txt --
Usage: tofu-age-encryption [--encrypt | --decrypt] [options]
  -age-identity-file string
    	age identity file
  -age-path string
    	path to age binary (default "@AGE_PROGRAM@")
  -age-recipient value
    	age recipient
  -age-recipients-file string
    	age recipients file
  -decrypt
    	decrypt payload
  -encrypt
    	encrypt payload
  -version
    	print version
