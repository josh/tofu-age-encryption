exec sh -c 'tofu-age-encryption --help | sed -E "s|default \"[^\"]*age\"|default \"MASK\"|"'
cmp stdout help.txt
! stderr .

-- help.txt --
Usage: tofu-age-encryption [--encrypt | --decrypt] [options]
  -age-path string
    	path to age binary (default "MASK")
  -decrypt
    	decrypt payload
  -encrypt
    	encrypt payload
  -identity string
    	age identity string, file:PATH or cmd:COMMAND
  -identity-command string
    	command whose output is the age identity
  -identity-file string
    	age identity file
  -recipient value
    	age recipient
  -recipients-file value
    	age recipients file
  -version
    	print version
